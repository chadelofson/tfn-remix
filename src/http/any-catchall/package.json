    "@tailwindcss/forms": "0.3.2",
    "aws-sdk": "2.796.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^6.0.0-beta.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^0.15.2",
    "@tailwindcss/jit": "0.1.18",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "autoprefixer": "10.2.5",
    "pm2": "4.5.6",
    "postcss": "8.2.9",
    "postcss-cli": "8.3.1",
    "tailwindcss": "2.0.4",
    "typescript": "^4.1.2"
